<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
		"http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

	<constant name="struts.devMode" value="true" />
	<constant name="struts.action.excludePattern" value="/VAADIN*,/vaadinapp*"/>

	<package name="basic-struts2-with-vaadin" extends="struts-default">

		<action name="index" class="com.infraleap.strutsapp.IndexAction" method="execute">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
		</action>

	</package>


</struts>